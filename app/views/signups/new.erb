<h2>New Signup</h2>

<%= form_for @signup do |f| %>
    
    <%= f.label :date %>
    <%= f.datetime_field :date %>
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, @users, :id, :first_name, :last_name, :email_address %>
    <%= f.label :place_id %>
    <%= f.collection_select :place_id, @places, :id, :name, :location, :hours_of_operation, :date %>
    <%= f.submit "Create Signup" %>

<% end %>