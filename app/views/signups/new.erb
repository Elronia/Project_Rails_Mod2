<h2>New Signup</h2>

<%= form_for @signup do |f| %>
    
    <%= f.label :date %>
    <%= f.datetime_field :date %>
    <%= f.label :user_id %>
    <%= f.collection_select :user_id, @users, :id, :first_name %>
    <%= f.label :place_id %>
    <%= f.collection_select :place_id, @places, :id, :name %>
    <%= f.submit "Create Signup" %>

<% end %>